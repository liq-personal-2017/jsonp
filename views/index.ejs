<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <script src="jsonp.js">
    </script>
    <script>
      jsonp({data:{jsonpdata:'jsonpdata'},url:'/api/jsonp',callback:'jsonpcallback'}).then(function (data){
        console.log(data);
      }).catch(function (e){
        console.log(e)
      }).always(function(){
        console.log('always');
      })
    </script>
  </body>
</html>
